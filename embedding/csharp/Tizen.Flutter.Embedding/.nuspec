<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>Tizen.Flutter.Embedding</id>
    <version>1.0.0</version>
    <authors>Samsung Electronics</authors>
    <description>Provides APIs for embedding Flutter into Tizen .NET apps.</description>
    <license type="expression">BSD-3-Clause</license>
    <projectUrl>https://github.com/flutter-tizen</projectUrl>
    <dependencies>
      <group targetFramework=".NETStandard2.0" />
      <group targetFramework="Tizen4.0" />
      <group targetFramework="Tizen9.0" />
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$configuration$\netstandard2.0\*.dll" target="lib\netstandard2.0" />
    <file src="bin\$configuration$\netstandard2.0\*.pdb" target="lib\netstandard2.0" />
    <file src="bin\$configuration$\tizen40\*.dll" target="lib\tizen40" />
    <file src="bin\$configuration$\tizen40\*.pdb" target="lib\tizen40" />
    <file src="bin\$configuration$\tizen90\*.dll" target="lib\tizen90" />
    <file src="bin\$configuration$\tizen90\*.pdb" target="lib\tizen90" />
    <file src="Tizen.Flutter.Embedding.targets" target="build" />
  </files>
</package>
